
@model IQueryable<Infra.Models.tbMemberPackage>
<div class="heading-section">

    <div class="sa-title popcat">Our Packages </div>
    <div class="heading-info">
        Let's see our packages
    </div>
    <div class="clear"></div>
</div>

@if (Model.Count() > 0)
{
    <div class="section-home available-car">

        <div class="default-carousel car-carousel">

            @foreach (var item in Model)
            {
                <!-- item -->
                <div class="item">
                    <div class="acr-box">
                        <div class="acr-box-in">
                            <div class="acr-img">
                                <img src="~/Plesirthemes/img/car2-nissan3.png" alt="you might like">
                            </div>
                            <div class="acr-content">
                                <div class="ct-name">@item.Title</div>
                                <div class="ct-cost"><span class="cprice"> @item.PackagePrice.Value.ToString("#,##0") MMK </span></div>

                                <div class="ct-reserve mt-10">
                                    <a href="car_single.html" class="theme-button">
                                        Detail
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="acr-bg">
                            <img src="~/Plesirthemes/img/bgcar.jpg">
                        </div>
                    </div>
                </div>
                <!-- .item -->

            }

        </div>
    </div>
}


