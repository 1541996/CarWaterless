    @section scripts{

        <link href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" rel="stylesheet" />
        <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
        <script src="~/Plesirthemes/js/stackedCards.js"></script>
        <script>

            var customerid = '@ViewBag.customerid'


            // JavaScript Document

         $(function () {
             $('.default-carousel').slick({
                 dots: false,
                 infinite: true,
                 speed: 300,
                 slidesToShow: 1,
                 centerMode: false,
                 arrows: false,
                 variableWidth: true
             });





            GetDailyHot();

            GetMemberPackages();
           // GetStackCardsMemberPackages();





        })


            function Packages(id) {
                $('.preloading').show();
                location.href = "../package?id=" + id + "&customerid=" + customerid;
            }

            function Services(type) {
                $('.preloading').show();
                location.href = "../service?type=" + type + "&customerid=" + customerid;
            }


        function GetBranchList()
        {
            $.ajax({
                cache: false,
                url: '@Url.Action("GetBranchList", "Home")',
                beforeSend: function () {

                },
                success: function (result) {
                    $('#div_branch').empty().append((result));
                },
                complete: function () {

                }
            });

         }


        function GetMemberPackages()
        {
            $.ajax({
                cache: false,
                url: '@Url.Action("GetMemberPackages", "Home")',
                beforeSend: function () {

                },
                success: function (result) {
                    $('#div_memberpackages').empty().append(result);
                },
                complete: function () {
                    $('.default-carousel').slick({
                        dots: false,
                        infinite: true,
                        speed: 300,
                        slidesToShow: 1,
                        centerMode: false,
                        arrows: false,
                        variableWidth: true
                    });


                    GetBranchList();
                }
            });

         }



        function GetDailyHot()
        {
            $.ajax({
                cache: false,
                url: '@Url.Action("GetDailyHot", "Home")',
                beforeSend: function () {

                },
                success: function (result) {
                    $('#div_dailyhot').empty().append(result);
                },
                complete: function () {
                    var $owl = $('.dailyhot-carousel');

                    $owl.children().each(function (index) {
                        $(this).attr('data-position', index); // NB: .attr() instead of .data()
                    });

                    $owl.owlCarousel({
                        center: true,
                        loop: true,
                        items: 2,
                    });

                    $(document).on('click', '.owl-item > div', function () {
                        // see https://owlcarousel2.github.io/OwlCarousel2/docs/api-events.html#to-owl-carousel
                        var $speed = 300;  // in ms
                        $owl.trigger('to.owl.carousel', [$(this).data('position'), $speed]);
                    });

                }
            });

         }


          function GetStackCardsMemberPackages()
        {
            $.ajax({
                cache: false,
                url: '@Url.Action("GetStackCardsMemberPackages", "Home")',
                beforeSend: function () {

                },
                success: function (result) {

                    $('#div_stackcardsmemberpackages').empty().append((result));
                 //   var stackcount = parseInt($('#stackcards_count').val());
                  //  var items = stackcount;
                  //  stackedCards();


                },
                complete: function () {

                    stackedCards();

                }
            });

         }


        function book() {
            $('.preloading').show();
          location.href='../book?customerid=@ViewBag.customerid';
        }

        function branch() {
            $('.preloading').show();
          location.href='../branch?customerid=@ViewBag.customerid';
        }

        function caradd() {
            $('.preloading').show();
          location.href='../car/add?ID=0&type=Add&customerid=@ViewBag.customerid';
        }

        function SeeMore(Id) {
            $('.preloading').show();
          location.href='../branch?customerid=@ViewBag.customerid';
        }


        </script>



    }
